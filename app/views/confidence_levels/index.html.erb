<h1>Listing confidence_levels</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @confidence_levels.each do |confidence_level| %>
  <tr>
    <td><%= link_to 'Show', confidence_level %></td>
    <td><%= link_to 'Edit', edit_confidence_level_path(confidence_level) %></td>
    <td><%= link_to 'Destroy', confidence_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
